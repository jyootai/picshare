<%= render "users/shared/info_display"%>
<div  id="user_photos" class="container">
	<div class="row">
		<%= render 'photos/shared/photo_display'%>	
	</div>

	<div id="photoModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
		<div class="modal-header">
			<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
			<h3 id="myModalLabel">Modal header</h3>
		</div>
	  <div class="modal-body">
		 <p>One fine body…</p>	
		</div>
		<div class="modal-footer">
			<button class="btn" data-dismiss="modal" aria-hidden="true">关闭</button>
			<button class="btn btn-primary">Save changes</button>
		</div>
	</div>

	
</div>
